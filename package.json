{
  "name": "stencil-slot-machine",
  "version": "0.0.1",
  "description": "A web component compiled with StencilJS to simulate a slot machine spin effect",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/stencil-slot-machine/stencil-slot-machine.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@stencil/core": "1.5.4"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/albertorcf/stencil-slot-machine.git"
  }
}
